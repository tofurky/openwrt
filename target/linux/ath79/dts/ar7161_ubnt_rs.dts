/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar7100.dtsi"

/ {
	compatible = "ubnt,rs", "qca,ar7161";
	model = "Ubiquiti Networks RouterStation";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x4000000>;
	};

	aliases {
		led-status = &rf;
	};

	chosen {
		bootargs = "console=ttyS0,115200n8";
	};

	extosc: ref {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-output-names = "ref";
		clock-frequency = <40000000>;
	};

	gpio-keys {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		sw4 {
			label = "sw4";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		rf: rf {
			label = "ubnt:green:rf";
			gpios = <&gpio 2 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
	};
};

&eth0 {
	status = "okay";

	phy-mode = "mii";

	mtd-mac-address = <&rbc 0x17>;

	fixed-link {
		speed = <100>;
		full-duplex;
	};
};

&eth1 {
	status = "okay";

	phy-mode = "rmii";

	mtd-mac-address = <&rbc 0x17>;
	mtd-mac-address-increment = <1>;

	fixed-link {
		speed = <100>;
		full-duplex;
	};
};

&pcie0 {
	status = "okay";
};

&spi {
	status = "okay";
	num-chipselects = <1>;

	flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "mxicy,mx25l12805d", "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "RedBoot";
				reg = <0x000000 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "kernel";
				reg = <0x030000 0x280000>;
			};

			partition@2b0000 {
				label = "rootfs";
				reg = <0x2b0000 0xd40000>;
			};

			partition@ff0000 {
				label = "FIS directory";
				reg = <0xff0000 0xf000>;
			};

			rbc: partition@fff000 {
				label = "RedBoot config";
				reg = <0xfff000 0x1000>;
				read-only;
			};
		};
	};
};

&uart {
	status = "okay";
};

&usb_phy {
	status = "okay";
};

&usb1 {
	status = "okay";
};

&usb2 {
	status = "okay";
};
